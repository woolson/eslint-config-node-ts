{
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "@typescript-eslint/interface-name-prefix": "off",
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/quotes": ["error", "single"],
    "@typescript-eslint/no-namespace": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "block-spacing": "error",
    "comma-dangle": ["error", "always-multiline"],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "func-call-spacing": ["error", "never"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "max-len": ["error", { "code": 200 }],
    "no-multi-spaces": ["error"],
    "no-multiple-empty-lines": "error",
    "no-trailing-spaces": "error",
    "object-curly-spacing": ["error", "always"],
    "semi": ["error", "always"],
    "space-before-blocks": "error",
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always"
    }],
    "no-unreachable": "error",
    "object-property-newline": ["error", {
      "allowAllPropertiesOnSameLine": false
    }],
    "object-curly-newline": [
      "error",
      {
        "multiline": true,
        "minProperties": 2
      }
    ],
    "key-spacing": ["error",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "camelcase": ["warn", {
      "allow": [
        "nonce_str",
        "mch_billno",
        "mch_id",
        "bill_type",
        "client_ip",
        "act_name",
        "send_name",
        "re_openid",
        "total_num",
        "total_amount",
        "scene_id",
        "risk_info",
        "out_trade_no",
        "media_id",
        "thumb_media_id",
        "action_name",
        "action_info",
        "expire_seconds",
        "scene_str",
        "redirect_uri",
        "response_type",
        "show_cover_pic",
        "content_source_url",
        "update_time",
        "total_count",
        "item_count",
        "page_size",
        "pra_channel",
        "pra_cust_categ1",
        "pra_cust_categ2",
        "sub_button",
        "template_id",
        "detail_id",
        "send_type",
        "hb_type",
        "send_time",
        "refund_time",
        "refund_amount",
        "rcv_time",
        "send_listid",
        "client_msg_id",
        "request_id",
        "character_string5",
        "character_string4",
        "character_string1",
        "character_string2",
        "sign_type",
        "transaction_id",
        "out_refund_no",
        "total_fee",
        "refund_fee",
        "return_code",
        "return_msg",
        "result_code",
        "result_msg",
        "err_code",
        "err_code_des",
        "session_key",
        "access_token",
        "grant_type",
        "js_code",
        "head_content",
        "tail_content",
        "card_id",
        "kf_account",
        "news_item",
        "expires_in",
        "ip_list",
        "created_at",
        "refresh_token",
        "subscribe_time",
        "tagid_list",
        "subscribe_scene",
        "qr_scene",
        "Day_1",
        "Hour_5",
        "Hour_1",
        "Min_10",
        "Min_5",
        "Min_1",
        "Sec_20",
        "Sec_10",
        "Sec_5",
        "formatted_address",
        "first_id",
        "first_name",
        "second_id",
        "second_name",
        "voice_send"
      ]
    }],
    "keyword-spacing": "error",
    "space-infix-ops": "error"
  }
}
